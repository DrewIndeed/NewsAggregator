<AnchorPane fx:id="anchorPane" prefHeight="860.0" prefWidth="803.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controller.ArticleViewController">
   <children>
      <BorderPane layoutX="-6.0" layoutY="-7.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <WebView fx:id="webView" prefHeight="200.0" prefWidth="622.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <HBox>
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#close" prefWidth="50.0" text="Close" />
                  <Button fx:id="previousButton" mnemonicParsing="false" onAction="#previous" prefWidth="100.0" text="Previous" />
                  <Button fx:id="nextButton" mnemonicParsing="false" onAction="#next" prefWidth="100.0" text="Next" />
